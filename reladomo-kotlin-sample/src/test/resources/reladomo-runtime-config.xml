<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<MithraRuntime>
    <ConnectionManager className="io.github.reladomokotlin.spring.connection.H2ConnectionManager">
        <Property name="connectionManagerName" value="default"/>
        <Property name="databaseName" value="testdb"/>
        <Property name="inMemory" value="true"/>
        
        <MithraObjectConfiguration className="io.github.reladomokotlin.sample.domain.Order" cacheType="partial"/>
        <MithraObjectConfiguration className="io.github.reladomokotlin.sample.domain.Customer" cacheType="partial"/>
        <MithraObjectConfiguration className="io.github.reladomokotlin.sample.domain.Product" cacheType="partial"/>
        <MithraObjectConfiguration className="io.github.reladomokotlin.sample.domain.OrderItem" cacheType="partial"/>
    </ConnectionManager>
</MithraRuntime>